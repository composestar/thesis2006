@proceedings{:middleware-aomd05,
    title           = {First workshop on Aspect Orientation for Middleware Development (AOMD 2005)},
    booktitle       = {First workshop on Aspect Orientation for Middleware Development (AOMD 2005)},
    month           = nov,
    year            = 2005,
    location        = {Grenoble, France},
    editor          = {Renaud Pawlak and Remi Douence},
    remark          = {pending},
    abstract        = {Middleware development requires numerous technical and business concerns to be taken into account and integrated as transparently as possible. Due to their ability to allow additional levels of separation of concerns, Aspect Oriented languages and frameworks (more generally Aspect Oriented Software Development  AOSD) show promise in not only development of middleware intensive applications but also development of middleware software itself. Thanks to AOSD, important middleware concerns such as security, distribution, persistence, transaction, and resource localization, can be programmed within separated modules (aspects), which can then be integrated through a weaving process.},
    url             = {http://www.lifl.fr/~pawlak/aomd/}
}

